{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "outDir": "dist",
    "baseUrl": "./src",
    "paths": {
      "@fixtures/*": ["fixtures/*"],
      "@pages/*": ["pages/*"],
      "@data/*": ["data/*"],
      "@types": ["utils/types.ts"],
    }
  },
  "include": ["src", "tests"]
}
